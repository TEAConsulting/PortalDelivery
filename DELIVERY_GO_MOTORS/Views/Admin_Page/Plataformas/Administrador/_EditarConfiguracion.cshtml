@model IEnumerable<ZPIDI_PORTAL.Models.C_Delivery_Plataformas_Configuracion>


<div class="modal-header">
    <h4 class="modal-title" id="exampleModalLabel"><strong>Configuración de plataforma - @Model.FirstOrDefault().C_Delivery_Plataformas.nombre_plataforma</strong></h4>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div class="modal-body">
    <center><h3>Porcentajes de comisión</h3></center>
    <table class="table table-striped">
        <tr>
            <th>Porcentaje ZPIDI</th>
            <th>Porcentaje Mensajeria</th>
            <th>Porcentaje Repartidor</th>
        </tr>
        @foreach (var item in Model)
        {
        <tr>
            <td><div contenteditable="true" class="div_editable" id="porc_zpidi">@item.porcentaje_comision_plat</div></td>
            <td><div contenteditable="true" class="div_editable" id="porc_msj">@item.porcentaje_comision_mens</div></td>
            <td><div contenteditable="true" class="div_editable" id="porc_repa">@item.porcentaje_comision_repa</div></td>
        </tr>
        }
    </table>

    <div class="text-center">
        <button class="btn btn-zpidi " onclick="ActualizarConfiguracionPlataforma(@Model.FirstOrDefault().id_pataforma_configuracion);">Actualizar configuración</button>
    </div>

</div>


