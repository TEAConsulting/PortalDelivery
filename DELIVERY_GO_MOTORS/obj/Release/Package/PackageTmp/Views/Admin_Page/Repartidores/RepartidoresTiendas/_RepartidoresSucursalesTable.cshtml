@model IEnumerable<ZPIDI_PORTAL.Models.C_Delivery_Repartidores_Empresas_Sucursales>

<div style="height:500px;overflow:scroll;overflow-x:hidden;">
    <table class="table table-bordered" id="datatable_sucursales_repa"
           data-toggle="table"
           data-search="true">
        <thead>
            <tr>
                <th>Codigo</th>
                <th>Nombre</th>
            </tr>
        </thead>
        <tbody>
            @if (Model.Count() > 0)
            {
                foreach (var item in Model.Select(x => x.C_Delivery_sucursales_gps).Distinct())
                {
                    <tr>
                        <td>@item.codigo_sucursal</td>
                        <td>@item.nombre_suc</td>
                    </tr>
                }
            }
            else
            {
                <tr><td>Sin sucursales</td><td></td></tr>
            }
        </tbody>

    </table>

</div>




